    <%- include("partails/header") %>
    <link rel="stylesheet" href="../../public/css/login.css" />

    <div class="content">
      <form action="/" method="post" class="loginPage">
        <div class="top">
          <h5>Hospital Management Portal, VIT Bhopal</h5>
          <div class="round">
            <p>H</p>
          </div>
        </div>
        <div class="login">
          <h1 class="text-secondary">LOGIN PAGE</h1>
          <h5>Enter your Institutional Credentials</h5>
          <input type="email"  name="username" placeholder="Enter your email" autocomplete="email" required autofocus />
          <i class="fas fa-envelope"></i>
          <input type="password" name="password"  class="password" placeholder="Enter your password" required autofocus autocomplete="current-password" >
          <i class="fas fa-eye-slash " id="eye"></i>
          <div>
            <button type="submit" class="btn lgBtn">Log In</button>
          </div>
        </div>
      </form>
    </div>
   
